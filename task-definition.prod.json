{
  "taskRoleArn": "ecs-hello-world-2-task-role-prod",
  "executionRoleArn": "ecs-hello-world-2-execution-role-prod",
  "containerDefinitions": [
    {
      "name": "hello-world-2",
      "image": "hello-world-2:prod",
      "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "resourceRequirements": null,
      "environment": [
        {
          "name": "ENVIRONMENT",
          "value": "prod"
        },
        {
          "name": "SERVICENAME",
          "value": "hello-world-2"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/hello-world-2/prod",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "hello-world-2-prod"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "inferenceAccelerators": [],
  "cpu": "256",
  "memory": "512",
  "family": "ecs-hello-world-2-task-definition-prod",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": []

}